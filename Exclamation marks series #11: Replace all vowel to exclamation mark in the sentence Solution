using namespace std;

string replace(const string &s)
{
    string str = s;
    string vowels = "aeiouAEIOU";
    for (int i = 0; i < s.size(); ++i)
    {
        for (int j = 0;j < vowels.size(); ++j)
        {
            if(str[i] == vowels[j])
            {
                str[i] = '!';
            }
        }
    }
    return str;
}
